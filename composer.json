{
    "name": "cable8mm/bigkinds-php-client",
    "description": "This library is PHP SDK for Bigkinds API",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "~6.0",
        "jdrieghe/array-helpers": ">=0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "laravel/pint": "^1.0",
        "fakerphp/faker": "^1.23"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Cable8mm\\BigkindsPhpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\BigkindsPhpClient\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/pint",
        "inspect": "./vendor/bin/pint --test",
        "apidoc": "doctum.phar update doctum.php --output-format=github --no-ansi --no-progress"
    }
}