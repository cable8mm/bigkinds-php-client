{
    "name": "cable8mm/bigkinds-php-client",
    "description": "This library is PHP SDK for Bigkinds API",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "~6.0",
        "jdrieghe/array-helpers": ">=0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "laravel/pint": "^1.10",
        "fakerphp/faker": "^1.23"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Cable8mm\\BigkindsPhpClient\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\BigkindsPhpClient\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests"
        ],
        "lint": [
            "./vendor/bin/pint"
        ]
    }
}
